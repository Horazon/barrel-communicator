<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" spacing="5"
      fx:controller="pl.horazon.fx.controllers.TypeSendBarController">

        <TextField fx:id="fxTxtMsg" HBox.hgrow="ALWAYS"/>
        <Button fx:id="fxBtnSend" text="%main.btn.send.txt" GridPane.columnIndex="1"
                onAction="#onClickSend"/>


    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</HBox>
